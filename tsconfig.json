{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "moduleResolution": "node",
    "paths": {
      "$root/*": ["./src/*"],
      "$lib/*": ["./src/functions*"],
      "$schemas": ["./src/schemas"],
      "$schemas/*": ["./src/schemas/*"],
      "$components/*": ["./src/components/*"]
    }
  }
}
